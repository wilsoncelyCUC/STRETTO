                                                  <% orchestra_link = Orchestra.find(invitation.orchestra_id)  %>
                                                  <%= link_to " ", orchestra_path(orchestra_link), class:"card-link-link"%>
                                                </h6>
                                            </div>
                                          </div>
                                      </div>
                                    </div>
                                <% end %>
                              </div>
                          </div>
                        </div>
                    </div>
