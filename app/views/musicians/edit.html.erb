<%= simple_form_for(@musician) do |f| %>

  <%= f.input :first_name %>

  <%= f.input :last_name %>

  <%= f.input :birthday %>

  <%= f.input :level %>

  <%= f.input :instrument %>

  <%= f.input :style %>

  <%= f.input :photo %>

  <%= f.input :bio %>

  <%= f.input :zip_code %>
  <%= f.submit %>
<% end %>
